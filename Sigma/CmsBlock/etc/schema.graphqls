type CmsBlockItem {
  identifier: String!
  is_active: Boolean!
  title: String!
  content: String!
}

type CmsBlocks {
  items: [CmsBlockItem!]!
}

type Query {
  cmsBlocks(identifiers: [String!]!): CmsBlocks @resolver(class: "\\Sigma\\CmsBlock\\Model\\Resolver\\CustomCmsBlocks") @doc(description: "Retrieve CMS blocks by identifiers")
}
